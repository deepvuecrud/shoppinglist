<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
</head>
<body>
    <div class="header">
        <div class="profile">
            <a class="portrait"><img src="~/Content/img/main.jpg" /></a>
            <div id="myName" class="name"></div>
            <div id="MyTitle" class="intro"></div>
            <div id="inMyToolBelt" class="intro"></div>
            <div class="links">
                <a class="icon" target="_blank" href="https://github.com/mrjeanjoseph" title="GitHub">
                    <span class="bi bi-linkedin">&#xe237;</span>
                </a>
                <a class="bi bi-facebook" target="_blank" href="https://www.linkedin.com/in/mrjeanjoseph/"
                   title="LinkedIn">
                    <span class="fab fa-2x fa-linkedin">&#xe252;</span>
                </a>
                <a class="icon" target="_blank" href="https://www.hackerrank.com/fjeanjoseph"
                   title="HackerRank"><span class="fab fa-2x fa-hackerrank">&#xe271;</span></a>
                <a class="icon" target="_blank" href="https://twitter.com/mrjeanjoseph" title="Twitter">
                    <span class="fab fa-2x fa-twitter-square">&#xe286;</span>
                </a>
            </div>
            <!-- Pick up where ytou left off -->
        </div>
        <nav class="navigation">
            <ul class="tabs" id="tabs">
                <li class="tab" target="_blank">
                    <a href="https://drive.google.com/file/d/168CBqVJ6kaDXsYAPYqc_cJsrLfOedwbr/view?usp=sharing">
                        Get CV/
                    </a>
                </li>
                <li class="tab" target="_blank">
                    <a href="https://drive.google.com/file/d/1E9w6Iz8eCuLSPP4NRbdhOPKPyQo3LAJk/view?usp=sharing">Certification</a>
                </li>
            </ul>
        </nav>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Jean-Joseph, This is my Portfolio</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
<script src="~/Scripts/jquerypopper.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.min.js" integrity="sha384-cuYeSxntonz0PPNlHhBs68uyIAVpIIOZZ5JqeqvYYIcEL727kskC66kF92t6Xl2V" crossorigin="anonymous"></script>
    @RenderSection("scripts", required: false)
</body>
</html>



<script type="text/javascript">
    function RefreshPage() {
        window.location.reload()
    }
</script>